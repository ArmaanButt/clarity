<h1>Courses#index</h1>
<p>Find me in app/views/courses/index.html.erb</p>
<h1>Listing articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @courses.each do |course| %>
    <tr>
      <td><%= course.course_code %></td>
      <td><%= course.course_prof %></td>
      <td><%= course.course_name %></td>
    </tr>
  <% end %>
</table>
